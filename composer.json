{
  "name": "openclassrooms/onesky-bundle",
  "description": "One Sky Bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\OneSkyBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\OneSkyBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "onesky/api-library-php5": "1.*",
    "symfony/dependency-injection": ">=2.7 <4",
    "symfony/console": ">=2.7 <4",
    "symfony/config": ">=2.7 <4",
    "symfony/finder": ">=2.7 <4",
    "symfony/framework-bundle": ">=2.7 <4",
    "symfony/http-kernel": ">=2.7 <4"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "1.*",
    "phpunit/phpunit": "~5.0",
    "satooshi/php-coveralls": "1.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
